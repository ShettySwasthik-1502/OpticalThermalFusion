{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/web/index.html" },
    { "src": "/(.*)", "dest": "/web/$1" },
    { "src": "/fuse", "dest": "/api/app.py" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "vercel-python@3.9"
    }
  }
}
